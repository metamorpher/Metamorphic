---
    name: "test-powershell"
    description: "Executes a powershell test script."
    enabled: true

    signal:
        id: "testpowershell"
        parameters:
            arguments: "undefined"

    action:
        id: "powershell"
        parameters:
            scriptfile: "test-powershell.ps1"
            arguments: "{{signal.arguments}}"